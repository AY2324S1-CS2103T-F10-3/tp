<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER_LEFT" minHeight="39">
    <padding>
        <Insets top="4" right="8" bottom="4" left="8" />
    </padding>
    <Label fx:id="name" alignment="CENTER" styleClass="cell_small_label" text="\$first" />
    <Label fx:id="ingredients" alignment="CENTER"  styleClass="cell_small_label" text="\$ingredients" />
    <Label fx:id="steps" alignment="CENTER"  styleClass="cell_small_label" text="\$steps" />
</VBox>
